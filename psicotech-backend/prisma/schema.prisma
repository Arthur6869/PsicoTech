generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int      @id @default(autoincrement())
  username     String   @unique
  email        String   @unique
  passwordHash String
  perfilTipo   String   @default("Estudante")
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
